{
    "name": "feexpay/feexpayPhp",
    "type": "library",
    "description": "Php sdk of Feexpay - Online payment solution by credit card and mobile money",
    "keywords": [
        "Feexpay",
        "FeexpayPhp"
    ],
    "homepage": "https://github.com/Feexpay/FeexpayPhp",
    "license": "MIT",
    "authors": [
        {
            "name": "Feexpay",
            "email": "contact@feexpay.me",
            "homepage": "https://feexpay.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Feexpay\\FeexpayPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feexpay\\FeexpayPhp\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
